<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.NodesDao">
    <select id="getNodes" resultType="pojo.Nodes">
        select * from nodes where product_id = #{productId} and price = #{price}
    </select>
</mapper>